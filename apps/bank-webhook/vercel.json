{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "buildCommand": "next build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "DATABASE_URL": "@DATABASE_URL",
      "SECRET_KEY": "@SECRET_KEY"
    }
  }
  