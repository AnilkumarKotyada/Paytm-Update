{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "DATABASE_URL": "@DATABASE_URL",
      "SECRET_KEY": "@SECRET_KEY"
    }
  }
  