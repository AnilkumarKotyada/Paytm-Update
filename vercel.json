{
    "version": 2,
    "builds": [
      {
        "src": "apps/bank-webhook/package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "apps/docs/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "package.json",
        "use": "turbo"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/apps/bank-webhook/dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/apps/docs/$1"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  