{
  "version": 2,
  "builds": [
    {
      "src": "apps/bank-webhook/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/user-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/bank-webhook/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/user-app/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
